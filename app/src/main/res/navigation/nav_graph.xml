            android:id="@+id/action_scanQrCodeFragment_to_showRoomEstimationFragment"
            app:destination="@id/showRoomEstimationFragment"
            app:popUpTo="@id/showRoomEstimationFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_scanQrCodeFragment_to_restaurantEstimationFragments"
            app:destination="@id/restaurantEstimationFragments"
            app:popUpTo="@id/restaurantEstimationFragments"
            app:popUpToInclusive="true" />
    </fragment>
    <action
        android:id="@+id/action_global_scanQrCodeFragment"
        app:destination="@id/scanQrCodeFragment"
        app:enterAnim="@anim/enter_anim"
        app:exitAnim="@anim/enter_anim"
        app:popEnterAnim="@anim/pop_enter_anim"
        app:popExitAnim="@anim/pop_exit_anim" />
