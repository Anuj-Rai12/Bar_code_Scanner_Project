            app:argType="com.example.mpos.data.cofirmDining.ConfirmDiningRequest"
            app:nullable="true" />
    </fragment>
    <fragment
        android:id="@+id/dealsFragment"
        android:name="com.example.mpos.ui.deals.DealsFragment"
        android:label="deals_fragment_layout"
