            app:destination="@id/confirmOderFragment"
            app:popEnterAnim="@anim/pop_enter_anim"
            app:popExitAnim="@anim/pop_exit_anim"
            app:popUpTo="@id/confirmOderFragment"
            app:popUpToInclusive="true" />
        <argument
            android:name="tbl"
            app:argType="com.example.mpos.data.table_info.model.json.TableDetail"
            app:nullable="true" />

        <argument
            android:name="list"
            app:argType="com.example.mpos.ui.searchfood.model.FoodItemList" />
        <argument
            android:name="confirmreq"
            app:argType="com.example.mpos.data.cofirmDining.ConfirmDiningRequest"
            app:nullable="true" />
        <action
            android:id="@+id/action_searchFoodFragment_to_costDashBoardFragment"
            app:destination="@id/costDashBoardFragment"
            app:popUpTo="@id/costDashBoardFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_searchFoodFragment_to_billingFragment"
            app:destination="@id/billingFragment"
            app:popUpTo="@id/billingFragment"
            app:popUpToInclusive="true" />
        <argument
            android:name="type"
            app:argType="string" />
        <action
            android:id="@+id/action_searchFoodFragment_to_showRoomEstimationFragment"
            app:destination="@id/showRoomEstimationFragment"
            app:popUpTo="@id/showRoomEstimationFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_searchFoodFragment_to_restaurantEstimationFragments"
            app:destination="@id/restaurantEstimationFragments"
            app:popUpTo="@id/restaurantEstimationFragments"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/scanQrCodeFragment"
        android:name="com.example.mpos.ui.scan.ScanQrCodeFragment"
        android:label="scan_qr_layout"
        tools:layout="@layout/scan_qr_layout">
        <argument
            android:name="item"
            app:argType="integer" />
        <action
            android:id="@+id/action_scanQrCodeFragment_to_testingConnectionFragment"
            app:destination="@id/testingConnectionFragment"
            app:popEnterAnim="@anim/pop_enter_anim"
            app:popExitAnim="@anim/pop_exit_anim"
            app:popUpTo="@id/scanQrCodeFragment"
            app:popUpToInclusive="true" />

        <action
            android:id="@+id/action_scanQrCodeFragment_to_confirmOderFragment"
            app:destination="@id/confirmOderFragment"
            app:popEnterAnim="@anim/pop_enter_anim"
            app:popExitAnim="@anim/pop_exit_anim"
            app:popUpTo="@id/confirmOderFragment"
            app:popUpToInclusive="true" />
        <argument
            android:name="tbl"
            app:argType="com.example.mpos.data.table_info.model.json.TableDetail"
            app:nullable="true" />
        <argument
            android:name="food"
            app:argType="com.example.mpos.ui.searchfood.model.FoodItemList"
            app:nullable="true" />
        <argument
            android:name="confirmreq"
            app:argType="com.example.mpos.data.cofirmDining.ConfirmDiningRequest"
            app:nullable="true" />
        <action
            android:id="@+id/action_scanQrCodeFragment_to_costDashBoardFragment"
            app:destination="@id/costDashBoardFragment"
            app:popUpTo="@id/costDashBoardFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_scanQrCodeFragment_to_billingFragment"
            app:destination="@id/billingFragment"
            app:popUpTo="@id/billingFragment"
            app:popUpToInclusive="true" />
        <argument
            android:name="type"
            app:argType="string" />
