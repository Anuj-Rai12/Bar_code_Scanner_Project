<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/splashScreenFragment">

    <fragment
        android:id="@+id/testingConnectionFragment"
        android:name="com.example.mpos.ui.testingconnection.TestingConnectionFragment"
        android:label="testing_connection_fragment"
        tools:layout="@layout/testing_connection_fragment">
        <argument
            android:name="bar"
            app:argType="com.example.mpos.data.login.model.TestingBarcodeConnection"
            app:nullable="true" />
        <action
            android:id="@+id/action_testingConnectionFragment_to_loginScreenFragment"
            app:destination="@id/loginScreenFragment"
            app:enterAnim="@anim/enter_anim"
            app:exitAnim="@anim/exit_anim"
            app:popEnterAnim="@anim/pop_enter_anim"
            app:popExitAnim="@anim/pop_exit_anim"
            app:popUpTo="@id/testingConnectionFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/tableManagementOrCostEstimate"
        android:name="com.example.mpos.ui.tableorcost.TableManagementOrCostEstimate"
        android:label="table_or_cost_layout"
        tools:layout="@layout/table_or_cost_layout">
        <action
            android:id="@+id/action_tableManagementOrCostEstimate_to_tableManagementFragment"
            app:destination="@id/tableManagementFragment"
            app:enterAnim="@anim/enter_anim"
            app:exitAnim="@anim/exit_anim"
            app:popEnterAnim="@anim/pop_enter_anim"
            app:popExitAnim="@anim/pop_exit_anim" />
        <action
            android:id="@+id/action_tableManagementOrCostEstimate_to_costDashBoardFragment"
            app:destination="@id/costDashBoardFragment"
            app:enterAnim="@anim/enter_anim"
            app:exitAnim="@anim/exit_anim"
            app:popEnterAnim="@anim/pop_enter_anim"
            app:popExitAnim="@anim/pop_exit_anim" />
        <action
            android:id="@+id/action_tableManagementOrCostEstimate_to_tableReservationFragment"
            app:destination="@id/tableReservationFragment"
            app:enterAnim="@anim/enter_anim"
            app:exitAnim="@anim/exit_anim"
            app:popEnterAnim="@anim/pop_enter_anim"
            app:popExitAnim="@anim/pop_exit_anim" />
        <action
            android:id="@+id/action_tableManagementOrCostEstimate_to_billingFragment"
            app:destination="@id/billingFragment"
            app:enterAnim="@anim/enter_anim"
            app:exitAnim="@anim/exit_anim"
            app:popEnterAnim="@anim/pop_enter_anim"
            app:popExitAnim="@anim/pop_exit_anim" />
        <argument
            android:name="information"
            app:argType="com.example.mpos.data.login.model.api.json.ApkLoginJsonResponse" />
        <action
            android:id="@+id/action_tableManagementOrCostEstimate_to_restaurantEstimationFragments"
            app:destination="@id/restaurantEstimationFragments"
            app:enterAnim="@anim/enter_anim"
            app:exitAnim="@anim/exit_anim"
            app:popEnterAnim="@anim/pop_enter_anim"
            app:popExitAnim="@anim/pop_exit_anim" />
        <action
            android:id="@+id/action_tableManagementOrCostEstimate_to_showRoomEstimationFragment"
            app:destination="@id/showRoomEstimationFragment"
            app:enterAnim="@anim/enter_anim"
            app:exitAnim="@anim/exit_anim"
            app:popEnterAnim="@anim/pop_enter_anim"
            app:popExitAnim="@anim/pop_exit_anim" />
        <action
            android:id="@+id/action_tableManagementOrCostEstimate_to_restaurantBillingFragment"
            app:destination="@id/restaurantBillingFragment"
            app:enterAnim="@anim/enter_anim"
            app:exitAnim="@anim/exit_anim"
            app:popEnterAnim="@anim/pop_enter_anim"
            app:popExitAnim="@anim/pop_exit_anim" />
        <action
            android:id="@+id/action_tableManagementOrCostEstimate_to_showRoomBillingFragment"
            app:destination="@id/showRoomBillingFragment"
            app:enterAnim="@anim/enter_anim"
            app:exitAnim="@anim/exit_anim"
            app:popEnterAnim="@anim/pop_enter_anim"
            app:popExitAnim="@anim/pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/tableManagementFragment"
        android:name="com.example.mpos.ui.tablemange.TableManagementFragment"
        android:label="table_mangment_layout"
        tools:layout="@layout/table_mangment_layout">
        <action
            android:id="@+id/action_tableManagementFragment_to_confirmOderFragment"
            app:destination="@id/confirmOderFragment"
            app:enterAnim="@anim/enter_anim"
            app:exitAnim="@anim/exit_anim"
            app:popEnterAnim="@anim/pop_enter_anim"
            app:popExitAnim="@anim/pop_exit_anim" />
        <argument
            android:name="store_name"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/confirmOderFragment"
        android:name="com.example.mpos.ui.oderconfirm.ConfirmOderFragment"
        android:label="confirm_order_layout"
        tools:layout="@layout/confirm_order_layout">
        <argument
            android:name="list"
            app:argType="com.example.mpos.ui.searchfood.model.FoodItemList"
            app:nullable="true" />
        <argument
            android:name="tbl"
            app:argType="com.example.mpos.data.table_info.model.json.TableDetail" />
        <argument
            android:name="confirmreq"
            app:argType="com.example.mpos.data.cofirmDining.ConfirmDiningRequest"
            app:nullable="true" />
    </fragment>
    <fragment
