        <argument
            android:name="list"
            app:argType="com.example.mpos.ui.searchfood.model.FoodItemList" />
        <argument
            android:name="tbl"
            app:argType="com.example.mpos.data.table_info.model.json.TableDetail"
            app:nullable="true" />
        <argument
